{
  "name": "豆瓣小组管理助手",
  "version": "1.0.3",
  "description": "简化封禁与删除操作、快速退出小组",
  "content_scripts": [
    {
      "matches": ["http://www.douban.com/group/topic/*"],
      "run_at": "document_end",
      "js": ["jquery-1.6.1.min.js", "group_topic.js"]
    },
    {
      "matches": ["http://www.douban.com/group/mine"],
      "run_at": "document_end",
      "js": ["jquery-1.6.1.min.js", "group_manage.js"]
    }],
  "icons": {
    "16": "appicon_16.png",
    "48": "appicon_48.png",
    "128": "appicon_128.png"
  }
}
