{
  "name": "豆瓣小组管理助手",
  "version": "1.0.0",
  "description": "简化封禁并删除操作、找出僵尸用户",
  "content_scripts": [
    {
      "matches": ["http://www.douban.com/group/topic/*"],
      "run_at": "document_end",
      "js": ["jquery-1.6.1.min.js", "group_topic.js"]
    },
    {
      "matches": ["http://www.douban.com/group/*/members*"],
      "run_at": "document_end",
      "js": ["jquery-1.6.1.min.js", "group_members.js"]
    }],
  "icons": {
    "16": "appicon_16.png",
    "48": "appicon_48.png",
    "128": "appicon_128.png"
  }
}